-transform: uppercase;\n    width: 603px;\n    text-shadow: 0px 3px 12px $black;\n    @include pad-xs {\n        font-size: 44px;\n    }\n    @include phone-xl {\n        width: 100%;\n        padding: 20px 0px;\n    }\n    @include phone-xs {\n        font-size: 26px;\n    }\n}\n.go {\n    a {\n        display: inline-block;\n        background: $yellow;\n        color: $black;\n        font-size: 26px;\n        text-decoration: none;\n        padding: 16px 35px;\n        border-radius: 9px;\n        text-transform: uppercase;\n        box-shadow: 0px 4px 14px rgba(0, 0, 0, 0.28);\n    }\n}\n.bootom_wrap {\n    display: flex;\n    flex-direction: column;\n    flex-wrap: wrap;\n}\n.bottom {\n    display: flex;\n    align-items: baseline;\n    justify-content: space-between;\n    width: 60%;\n    border-bottom: 1px solid $white;\n    padding-bottom: 10px;\n    @include pad-xl {\n        width: 70%;\n    }\n    @include phone-xl {\n        margin: 0 auto;\n    }\n    @include phone-xs{\n        align-items: center;\n    }\n    .cp {\n        color: $white;\n        text-transform: uppercase;\n        width: 192px;\n        @include phone-xs {\n            text-align: initial;\n        }\n    }\n    .games {\n        @include pad-xl {\n            width: 35%;\n        }\n        @include pad-xs {\n            width: 50%;\n        }\n    \n        @include phone-xl {\n            width: 40%;\n        }\n    }\n}\n.accept {\n    display: flex;\n    flex-wrap: wrap;\n    align-items: center;\n    width: 60%;\n    justify-content: stretch;\n    padding-top: 20px;\n    img {\n        height: 20px;\n        padding: 10px 20px;\n        @include phone-xl{\n            height: 14px;\n        }\n        @include phone-xs {\n            padding: 6px 13px;\n        }\n    }\n    @include pad-xl {\n        width: 75%;\n    }\n    @include phone-xl {\n        justify-content: center;\n        width: 80%;\n        margin: 0 auto;\n    }\n}\nfooter {\n    background: $blue;\n    .wrap{\n        width: 690px;\n        margin: 0 auto;\n        @include phone-xl {\n            width: 400px;\n        }\n        @include phone-sm {\n            width: 300px;\n        }\n        p {\n            text-align: center;\n            font-family: 'Montserrat', sans-serif;\n            color: $white;\n            font-size: 14px;\n            padding: 20px 15px;\n            margin: 0;\n            @include phone-sm {\n                font-size: 10px;\n            }\n        }\n    }\n}"},"lineCount":null}},"error":null,"hash":"93e5030ef84b3c3c06da3b2234379027","cacheData":{"env":{}}}